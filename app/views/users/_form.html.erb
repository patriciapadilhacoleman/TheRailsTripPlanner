
<%= form_for(@user) do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.text_field :password %>


    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <br>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <br>
    <%= f.label :email%>
    <%= f.text_field :email %>
    <%= f.label :phone %>
    <%= f.text_field :phone %>

    <br>

    <%= f.label :home_airport %>
    <%= f.text_field :home_airport %>
    <%= f.label :dob %>
    <%= f.text_field :dob %>

    <br>

    <%= f.label :street_address %>
    <%= f.text_field :street_address %>
    <br>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :zip %>
    <%= f.text_field :zip %>
    <br>
    <%= f.submit class:"submit-btn" %>
<% end %>
